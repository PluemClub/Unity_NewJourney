using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class ObjectSpinVertical : MonoBehaviour
{
    [SerializeField] float CoinMoveSpX;
    [SerializeField] float CoinMoveSpY;
    [SerializeField] float CoinMoveSpZ;

    void Update()
    {
        transform.Rotate(360 * CoinMoveSpX * Time.deltaTime, 360 * CoinMoveSpY * Time.deltaTime, 360 * CoinMoveSpZ * Time.deltaTime);
    }
}
